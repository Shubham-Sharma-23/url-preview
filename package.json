{
  "name": "URLPreview",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' NODE_ENV=development next dev",
    "build": "next build",
    "start": "npm run build && next start",
    "prettier": "cross-env prettier  --check '**/*.{js,jsx}' ",
    "prettier:fix": "cross-env prettier  --check --write '**/*.{js,jsx}' ",
    "lint": "cross-env eslint --ext .jsx,.js",
    "lint:fix": "cross-env eslint --ext .jsx,.js --fix",
    "test": "cross-env jest --config config/jest/jest.config.js",
    "test:coverage": "cross-env jest --coverage --config config/jest/jest.config.js",
    "test:watch": "cross-env jest --coverage --watchAll --config config/jest/jest.config.js",
    "test:debug": "cross-env jest --runInBand --coverage --watchAll --config config/jest/jest.config.js",
    "storybook": "start-storybook -c config/storybook"
  },
  "dependencies": {
    "@types/jest": "^26.0.14",
    "bootstrap": "^4.5.2",
    "next": "9.5.2",
    "rc-slider": "^9.4.1",
    "react": "16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-calendar": "^3.1.0",
    "react-dom": "16.13.1",
    "react-hook-form": "^6.8.2",
    "react-select": "^3.1.0",
    "redux-saga": "^1.1.3",
    "sass": "^1.26.10",
    "use-saga-reducer": "^3.0.0",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@storybook/addon-actions": "^6.0.19",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.3.20",
    "@storybook/addon-storysource": "^6.0.19",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^6.0.19",
    "@testing-library/react": "^10.4.9",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "classnames": "^2.2.6",
    "colors": "^1.4.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "jest": "^26.4.2",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1"
  },
  "pre-commit": [
    "prettier:fix",
    "lint:fix"
  ],
  "pre-push": [
    "prettier:fix",
    "lint:fix",
    "test",
    "build"
  ]
}
